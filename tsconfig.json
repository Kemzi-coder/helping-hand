{
	"compilerOptions": {
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"paths": {
			"@images/*": [
				"./src/assets/images/*"
			],
			"@fonts/*": [
				"./src/assets/fonts/*"
			],
			"@components/*": [
				"./src/components/*"
			],
			"@utils/*": [
				"./src/utils/*"
			],
			"@views/*": [
				"./src/views/*"
			],
			"@models/*": [
				"./src/models/*"
			],
			"@customTypes/*": [
				"./src/types/*"
			],
			"@store/*": [
				"./src/store/*"
			]
		}
	}
}